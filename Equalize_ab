/*You are given two numbers AA and BB along with an integer XX. In one operation you can do one of the following:

Set A = A + X and B = B - XB=Bâˆ’X
Set A = A - X and B = B + XB=B+X  

Using above make a and b equal.
*/

#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	
	while(t--){
	    int a, b, x;
	    cin>>a>>b>>x;
	    
	    if(abs(a-b)%(2*x)==0 || a==b){
	        cout<<"yes"<<"\n";
	    }
	    else cout<<"no"<<"\n";
	}
	return 0;
}
